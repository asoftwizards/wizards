	<ele id="lname" class="StringInput" type="LOGIN" visibility="vo" field="Login"/>
	<ele id="fname" class="StringInput" type="LOGIN" visibility="vo" field="Login"/>
	<ele id="sname" class="StringInput" type="LOGIN" visibility="vo" field="Login"/>
