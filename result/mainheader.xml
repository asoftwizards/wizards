		<evt aid="popupcall" type="WM_POPUP" id="properties" src="/act/GPN/properties"/>
	</a>
	<lay>
		<grid id="UserParams">
			<row id="hr">
			  <cell id="ho"><ele id="Logo"/><ele id="nmm"></ele><ele id="nmm1"></ele></cell><cell id="ho1"><ele id="TokenName"/></cell>
			</row>
		</grid>
		<ele id="ServerEventListener" class="ServerEventListener" visibility="no"/>
		<ele id="ProgressBarMgrClass" visibility="no" class="ProgressBarMgrClass"/>
